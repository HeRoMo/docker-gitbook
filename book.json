{
  "title": "YOUR BOOK TITLE",
  "plugins": [
    "lb2br",
    "plantuml-cloud",
    "-sharing",
    "hide-published-with",
    "footnote-string-to-number",
    "anchors",
    "image-captions"
  ],
  "pluginsConfig": {
    "plantuml-cloud": {
      "protocol": "http",
      "host": "plantuml",
      "port": 1608,
      "blockRegex": "^```p?uml((.*[\r\n])+?)?```$"
    },
    "image-captions": {
      "caption": "Fig. _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
    }
  },
  "pdf": {
    "paperSize": "a4"
  },
  "language": "ja"
}
